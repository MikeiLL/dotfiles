{
  "files.associations": {
    "Brewfile": "ruby",
    "gitconfig.*": "ini",
    "*.js.symlink": "javascript",
    "*.json.symlink": "json",
    "*.symlink": "shellscript"
  },
  "eslint.enable": false,
  "editor.formatOnSave": true,
  "cSpell.words": ["brewfile", "dotfiles", "gitignore", "symlink", "symlinked", "symlinks", "tweetbot"]
}
